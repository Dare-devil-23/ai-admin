{
  "name": "ai-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "robotTextGenerator": "node ./scripts/generateRobotsTxt.ts",
    "createConfig": "node ./scripts/createConfig.ts",
    "dev": "set NODE_ENV=local && npm run createConfig && npm run robotTextGenerator && next dev -p 3000",
    "build": "set NODE_ENV=prod && npm run createConfig && npm run robotTextGenerator && next build",
    "build:qa": "export NODE_ENV=qa && npm run createConfig && npm run robotTextGenerator && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-tabs": "^1.1.4",
    "@uiw/react-md-editor": "^4.0.5",
    "axios": "^1.8.4",
    "clsx": "^2.1.1",
    "firebase": "^11.6.0",
    "katex": "^0.16.22",
    "lucide-react": "^0.488.0",
    "next": "15.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}
